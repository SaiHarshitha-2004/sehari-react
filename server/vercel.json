{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  { 
    "rewrites" : [
      { 
        "source" : "/(.*)" ,
        "destination" : "/"
      }
    ]
  }
  "routes": [
    {
      "source": "/(.*)",
      "destination": "server.js"
    }
  ]
}
